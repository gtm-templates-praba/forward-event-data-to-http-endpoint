// APIs
const getAllEventData = require('getAllEventData');
const getContainerVersion = require('getContainerVersion');
const getRequestHeader = require('getRequestHeader');
const JSON = require('JSON');
const logToConsole = require('logToConsole');
const sendHttpRequest = require('sendHttpRequest');


// Configuration and setup
const containerVer = getContainerVersion();
const isDebug = containerVer.debugMode;
const traceId = getRequestHeader('trace-id');

// Fields
const endpointUrl = data.endpointUrl;

// Headers
const headers = {'Content-Type': 'application/json'};

// Get all event data
let eventData = getAllEventData();

// Add endpoint URL to the payload
eventData.http_endpoint_url = endpointUrl;

// Process any hyphens in keys
eventData = convertKeys(eventData);

// Prepare request
const postBody = JSON.stringify(eventData);
const requestOptions = {
  headers: headers, 
  method: 'POST'
};

// Log request if in debug mode
// Adhere Stape's log pattern as described at https://github.com/snowplow/snowplow-gtm-server-side-tag/issues/6
if (isDebug) {
  logToConsole(JSON.stringify({
    'Name': 'JsonRequest',
    'Type': 'Request',
    'TraceId': traceId,
    'RequestMethod': 'POST',
    'RequestUrl': endpointUrl,
    'RequestBody': eventData,
  }));
}

// Send the request
sendHttpRequest(endpointUrl, (statusCode, headers, body) => {
  // Log response if in debug mode
  if (isDebug) {
    logToConsole(JSON.stringify({
      'Name': 'JsonRequest',
      'Type': 'Response',
      'TraceId': traceId,
      'ResponseStatusCode': statusCode,
      'ResponseHeaders': headers,
      'ResponseBody': body,
    }));
  }
  
  // Standard HTTP status code handling
  if (statusCode >= 200 && statusCode < 300) {
    data.gtmOnSuccess();
  } else {
    data.gtmOnFailure();
  }
}, requestOptions, postBody);

// Utility function to replace hyphens with underscores in keys
function convertKeys(obj) {
  const result = {};
  
  for (let key in obj) {
    if (obj.hasOwnProperty(key)) {
      // Replace hyphens with underscores in keys
      const newKey = key.split("-").join("_");
      
      // Handle nested objects recursively
      if (typeof obj[key] === 'object' && obj[key] !== null) {
        result[newKey] = convertKeys(obj[key]);
      } else {
        result[newKey] = obj[key];
      }
    }
  }
  
  return result;
}


